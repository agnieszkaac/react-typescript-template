{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "baseUrl": "./",
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "jsx": "react",
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
